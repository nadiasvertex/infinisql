Import(['env', 'libraries'])

isql_env = env.Clone()
isql_env.Append(LIBS=libraries + 
               ['protobuf', 
                'tbb', 
                'lmdb', 
                'pthread', 
                'zmq', 
                'lz4'])
          
server_source = ['main.cc']

isql_env.Program('infinisqld', server_source)
